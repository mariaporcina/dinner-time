      "package": "com.mariaporcina.dinnertime"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5920d1e7-c308-49eb-9473-b4b5e5712e37"
      }
